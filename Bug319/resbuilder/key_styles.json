{
	"blocks": {
		"Theme": {
			
		},
		"DarkTheme": {
			"include": [ "Theme" ],
			"preferenceTheme": "@style/AppTheme._Preference",
			"android:listChoiceIndicatorMultiple": "@drawable/custom_btn_check_holo_dark"
		},
		"LightTheme": {
			"include": [ "Theme" ],			
			"preferenceTheme": "@style/AppTheme._Preference.Light",
			"android:listChoiceIndicatorMultiple": "@drawable/custom_btn_check_holo_light"
		}
	},
	"data": {
		"AppTheme": {
			"parent": "Holo.Theme",
			"include": [ "DarkTheme" ]
		},
		"AppTheme.Light": {
			"parent": "Holo.Theme.Light",
			"include": [ "LightTheme" ]
		},
		"AppTheme.Light.DarkActionBar": {
			"parent": "Holo.Theme.Light.DarkActionBar",
			"include": [ "LightTheme" ]
		},
		"AppTheme._Preference": {
			"parent": "Holo.PreferenceTheme",
			"include": [ "DarkTheme" ]
		},
		"AppTheme._Preference.Light": {
			"parent": "Holo.PreferenceTheme.Light",
			"include": [ "LightTheme" ]
		}
	},
	"output": "values/holo_styles.xml"
}